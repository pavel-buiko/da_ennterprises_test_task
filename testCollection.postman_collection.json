							"// Сохраняем refreshToken из cookies\r",
							"const refreshCookie = pm.cookies.get(\"refreshToken\");\r",
							"if (refreshCookie) {\r",
							"    pm.environment.set(\"refreshToken\", refreshCookie);\r",
							"}\r",
							"\r",
							"// Сохраняем accessToken из тела ответа\r",
							"const responseJson = pm.response.json();\r",
